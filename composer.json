{
    "name": "aaemnnosttv/wp-cli-media-generate-command",
    "description": "Generate media (attachments + files) using WP-CLI",
    "type": "wp-cli-package",
    "homepage": "https://github.com/aaemnnosttv/wp-cli-media-generate-command",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WP_CLI_MediaGenerate\\": "src"
        },
        "files": [ "command.php" ]
    },
    "require": {
        "php": ">=5.6",
        "wp-cli/wp-cli": ">=1.0"
    },
    "require-dev": {
        "behat/behat": "~2.5",
        "wp-cli/scaffold-package-command": "^0.5.0",
        "wp-cli/wp-cli-bundle": "^2.0"
    },
    "scripts": {
        "run-behat": "behat --ansi",
        "update-readme": "wp scaffold package-readme $PWD --force"
    },
    "extra": {
        "commands": [
            "media generate image"
        ]
    },
    "config": {
        "platform": {
            "php": "5.6.31"
        }
    }
}
